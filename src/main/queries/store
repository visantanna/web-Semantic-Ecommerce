PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX foaf:  <http://xmlns.com/foaf/0.1#>
PREFIX ep:  <http://www.semanticweb.org/ACH2098/>
PREFIX epsx:  <http://www.semanticweb.org/ACH2098#>
PREFIX epx:  <http://www.semanticweb.org/ACH2098/#>

SELECT   ?store ?activity ?isLocatedIn ?represents ?storeLink ?storePhoto ?storeID ?storeName
{
?store rdf:type epx:Store;
epsx:activity ?activity;
ep:isLocatedIn ?isLocatedIn;
ep:represents ?represents;
epsx:storeLink ?storeLink;
epsx:storePhoto ?storePhoto;
ep:storeID ?storeID;
ep:storeName ?storeName;
}
ORDER BY ASC(?ind)
